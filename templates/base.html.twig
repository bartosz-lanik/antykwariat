<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% block description %}{% endblock %}
    <title>{% block title %}{{ site_name }}{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('signin.css', 'css') }}">
        <link rel="stylesheet" href="{{ asset('tagsinput.css', 'css') }}">
        <link rel="stylesheet" href="{{ asset('bootstrap.min.css', 'css') }}">
        <link rel="stylesheet" href="{{ asset('simpleLightbox.min.css', 'css') }}">
        <link rel="stylesheet" href="{{ asset('style.css', 'css') }}">
        <link rel="stylesheet" href="{{ asset('fonts/roboto.css', 'css') }}">
    {% endblock %}
    {% block metaDownload %}{% endblock %}
</head>
<body class="d-flex flex-column min-vh-100">
{% include 'components/header.html.twig' %}
{% block body %}{% endblock %}
{% include 'components/footer.html.twig' %}
{% block javascripts %}
    <script src="{{ asset('jquery-3.5.1.min.js', 'js') }}"></script>
    <script src="{{ asset('bootstrap.bundle.min.js', 'js') }}"></script>
    <script src="{{ asset('fontawesome.js', 'js') }}"></script>
    <script src="{{ asset('simpleLightbox.min.js', 'js') }}"></script>
    <script src="{{ asset('script.js', 'js') }}"></script>
{% endblock %}
</body>
</html>